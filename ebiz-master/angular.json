{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ebiz-master": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "standalone": false
        },
        "@schematics/angular:directive": {
          "standalone": false
        },
        "@schematics/angular:pipe": {
          "standalone": false
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/ebiz-master",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
"styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "./node_modules/bootstrap/dist/css/bootstrap.min.css",
              "./node_modules/ngx-bootstrap/datepicker/bs-datepicker.css",
              "src/assets/StyleSheetTOP.css",
              "src/assets/dashforge.css",
              "node_modules/sweetalert2/src/sweetalert2.scss",
              "src/assets/extensions/toastr/toastr.css",
              "node_modules/animate.css/animate.css",
              "src/styles.css",
              "src/assets/app-assets/vendors/css/forms/selects/select2.min.css"
            ],
            "scripts": [
              "src/assets/app-assets/vendors/js/vendors.min.js",
              "src/assets/app-assets/vendors/js/ui/jquery.sticky.js",
              "src/assets/app-assets/vendors/js/charts/jquery.sparkline.min.js",
              "src/assets/app-assets/vendors/js/forms/icheck/icheck.min.js",
              "src/assets/app-assets/vendors/js/forms/validation/jqBootstrapValidation.js",
              "src/assets/app-assets/vendors/js/ui/prism.min.js",
              "src/assets/app-assets/js/core/app-menu.js",
              "src/assets/app-assets/js/core/app.js",
              "src/assets/app-assets/js/scripts/ui/breadcrumbs-with-stats.js",
              "src/assets/app-assets/js/scripts/forms/form-login-register.js",
              "src/assets/app-assets/vendors/js/forms/select/select2.full.min.js",
              "src/assets/app-assets/js/scripts/forms/select/form-select2.js",
              "node_modules/jquery/dist/jquery.min.js",
              "src/assets/extensions/toastr/toastr.js",
              "src/assets/app-assets/vendors/js/extensions/moment.min.js",
              "src/assets/isNumber.js",
              "./node_modules/exceljs/dist/exceljs.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "ebiz-master:build:production"
            },
            "development": {
              "buildTarget": "ebiz-master:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "src/proxy.conf.js"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "ebiz-master:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": [],
            "karmaConfig": "karma.conf.js"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "72ed2596-2f0b-412d-9083-5d7cfc8ca8f1"
  }
}